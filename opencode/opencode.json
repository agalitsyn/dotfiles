{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "oh-my-opencode@latest"
  ],
  "default_agent": "default",
  "agent": {
    "clever": {
      "mode": "primary",
      "model": "anthropic/claude-opus-4-6",
      "description": "High quality reasoning with Claude Opus 4.6"
    },
    "default": {
      "mode": "primary",
      "model": "anthropic/claude-sonnet-4-6",
      "description": "Balanced with Claude Sonnet 4.6"
    },
    "fast": {
      "mode": "primary",
      "model": "anthropic/claude-haiku-4-5",
      "description": "Quick responses with Claude Haiku 4.5"
    },
    "made-in-china": {
      "mode": "primary",
      "model": "openrouter/moonshotai/kimi-k2.5",
      "description": "Kimi K2.5 via OpenRouter"
    }
  },
  "provider": {
    "openrouter": {
      "models": {
        "moonshotai/kimi-k2.5": {
          "name": "Kimi K2.5",
          "limit": {
            "context": 262144,
            "output": 65536
          }
        }
      }
    }
  }
}
