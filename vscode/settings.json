{
    "editor.fontFamily": "Hack, FiraCode-Retina, Source Code Pro, Go Mono, Monaco",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "editor.renderWhitespace": "none",
    "editor.rulers": [120],
    "editor.minimap.enabled": false,
    "problems.decorations.enabled": false,
    "terminal.integrated.shell.osx": "bash",
    "terminal.integrated.shell.linux": "bash",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.external.linuxExec": "x-terminal-emulator",
    "files.associations": {
        "Vagrantfile": "ruby"
    },
    "go.buildOnSave": "package",
    "go.lintOnSave": "package",
    "go.vetOnSave": "package",
    "go.buildTags": "",
    "go.buildFlags": [],
    "go.lintTool": "megacheck",
    "go.lintFlags": [],
    "go.vetFlags": [],
    "go.addTags": {
        "tags": "json",
        "options": "json=omitempty",
        "promptForTags": true,
        "transform": "snakecase"
    },
    "go.coverOnSave": false,
    "go.useCodeSnippetsOnFunctionSuggest": false,
    "go.formatTool": "goreturns",
    "go.gopath": "~",
    "go.gocodeAutoBuild": false,
    "workbench.iconTheme": null,
    "workbench.startupEditor": "none",
    "workbench.colorCustomizations": {
        "statusBar.background": "#242424",
        "statusBar.noFolderBackground": "#242424",
        "statusBar.debuggingBackground": "#242424"
    },
    "workbench.statusBar.feedback.visible": false,
    "extensions.autoUpdate": false,
    "update.channel": "none",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true
}

