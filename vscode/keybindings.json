[
    // enter, ctrl + enter to open, cmd+enter to rename
    {
      "key": "cmd+enter",
      "command": "renameFile",
      "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
      "key": "enter",
      "command": "-renameFile",
      "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
      "key": "enter",
      "command": "list.select",
      "when": "listFocus && !inputFocus"
    }
]
