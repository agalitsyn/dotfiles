[
    // enter to open, ctrl + enter to open in split, cmd + enter to rename
    {
      "key": "cmd+enter",
      "command": "renameFile",
      "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
      "key": "enter",
      "command": "-renameFile",
      "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
      "key": "enter",
      "command": "list.select",
      "when": "listFocus && !inputFocus"
    }
]
