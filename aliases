alias vi='vim'
alias less='less -Ri'
alias bashrc='vim ~/.bashrc && source ~/.bashrc'
alias rsync='rsync --exclude .svn'

alias pswd-gen='php ~/.pswd-gen'
alias vhost='php ~/.vhost'

alias gc="google-chrome --new-window"
alias my-ip="ifconfig eth0 | grep 'inet addr:' | cut -d: -f2 | awk '{ print $1}'"
